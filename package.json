{
  "name": "nisp",
  "version": "0.0.1",
  "description": "a non-Turing-complete lisp-0",
  "main": "lib",
  "scripts": {
    "no": "no",
    "prepublish": "no build"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysmood/nisp.git"
  },
  "keywords": [
    "lisp"
  ],
  "author": "http://ysmood.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysmood/nisp/issues"
  },
  "homepage": "https://github.com/ysmood/nisp#readme",
  "devDependencies": {
    "eslint": "^1.10.2",
    "junit": "1.0.0",
    "nokit": "0.16.7"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "double"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": [
        0
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }
}

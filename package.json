{
  "name": "nisp",
  "version": "0.7.0",
  "description": "A language that for easily build cross-language language",
  "main": "core/index.js",
  "scripts": {
    "no": "no",
    "test": "no test",
    "prepublish": "no build"
  },
  "files": [
    "core",
    "fn",
    "lang"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysmood/nisp.git"
  },
  "keywords": [
    "lisp"
  ],
  "author": "http://ysmood.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysmood/nisp/issues"
  },
  "homepage": "https://github.com/ysmood/nisp#readme",
  "devDependencies": {
    "eslint": "3.4.0",
    "junit": "1.4.4",
    "nokit": "0.22.28",
    "yaku": "0.16.6"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "double"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": [
        0
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }
}
